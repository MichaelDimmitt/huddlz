{
  "context_servers": {
    "Context7": {
      "command": {
        "path": "npx",
        "args": ["-y", "@upstash/context7-mcp@latest"]
      },
      "settings": {}
    },
    "tidewave": {
      "command": {
        "path": "mcp-proxy",
        "args": ["http://localhost:4000/tidewave/mcp"],
        "env": null
      },
      "settings": {}
    }
  }
}
